<div class="sent_email">
  <div class="content_top">
    <div class="primary_heading">
      <h1>Listing sent emails</h1>
    </div>
  </div>
  <!--content_top-->
  <div class="content_middle">
    <div class="grid_table">
      <table cellpadding="0" cellspacing="0" class="table_listing emails_listing">
        <colgroup>
          <col class="one"/>
          <col class="two"/>
          <col class="three"/>
          <col class="four"/>
        </colgroup>
        <thead>
        <tr>
          <th class="align_left">Subject</th>
          <th class="align_left">Recipient</th>
          <th class="align_center">Type</th>
          <th class="align_center">Date</th>
        </tr>
        </thead>
        <tbody id="email_body">
        <%= render 'sent_emails' %>
        </tbody>
        <tfoot id="paging_links">
        <%= render "paging" %>
        </tfoot>
      </table>
    </div>
    <!--grid_table-->
  </div>
  <!--content_middle-->
</div><!--sent_email-->
