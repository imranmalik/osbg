<div class="report_header">
  <div class="report_header_content">
    <div class="report_date">
      <div class="control_name">
        <p>Date Range</p>
      </div>
      <input name="fromDate" type="text" id="fromDate" value=""/>
      <p>to</p>
      <input name="toDate" type="text" id="toDate"  value="" />
    </div>
    <div class="report_clients">
      <div class="control_name">
        <p>Clients</p>
      </div>
      <%= collection_select "criteria", "client_id", Client.all, :id, :organization_name, {:prompt => ""},{"data-placeholder" => "Select a client...", :class => "client chzn-select"} %>
      <div class="control_name">
        <p>Type</p>
      </div>
      <%= select_tag "criteria[payment_method]", PAYMENT_METHODS, {:class => "payment-method chzn-select"} %>
    </div>
    <div class="report_update">
      <a href="#">Update</a>
    </div>



