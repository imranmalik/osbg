<div class="report_content_container">

  <div class="report_content_header">
    <div class="report_content_heading">
      <h1>Revenue By Client</h1>
    </div>
  </div>
  <!-- report_content_header -->

  <div class="content_header_data">
    <div class="header_data_left">
      <div class="header_data_detail">
        <ul>
          <li class="header_name"><%= @report.client_name %></li>
          <li class="header_duration"><%= @report.period %></li>
        </ul>
      </div>
    </div>
  </div>
  <!-- content_header_data -->

  <div class="table_session">
    <table cellspacing="0" cellpadding="0" class="table_listing">
      <colgroup>
        <col class="first">
        <col class="second">
        <col class="third">
        <col class="forth">
        <col class="fifth">
        <col class="sixth">
      </colgroup>
      <tr>
        <th class="align_left">Client</th>
        <% 12.times do |month| %>
            <th class="align_right"><%= Date::MONTHNAMES[month+1].to_s[0..2] %></th>
        <% end %>
        <th class="align_right">Total</th>
      </tr>
      <% @report.report_data.each do |revenue| %>
          <!--tr><td colspan="6"><%#= payment.inspect %></td> </tr-->
          <tr>
            <td class="align_center"><%= revenue.organization_name %></td>
            <% 12.times do |month| %>
                <td class="align_rirght"><%= number_to_currency(revenue["#{Date::MONTHNAMES[month+1]}"]) %></td>
            <% end %>
            <td class="align_right"><%= number_to_currency(revenue.client_total) %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
  </div>
  <!-- table_session -->

  <div class="report_footer">
    <div class="total_payment">
      <ul>
        <li class="total_payment_title">Total Payments Received:</li>
        <li>$ <%= @report.report_total || "Total object missing" %> <span>usd</span></li>
      </ul>
    </div>
    <div class="footer_note">
      <p>* Payments applied from credit do not count towards the total</p>
    </div>
  </div>
  <!-- report_footer -->

</div>
<!-- report_content_container -->