<%- model_class = Payment -%>
<div id="invoices_container">default data</div>
<div class="page-header">
  <h1><%= t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>

<table class="table table_listing payment_listing">
  <thead>
  <th>Invoice</th>
  <th>Client</th>
  <th>Payment method</th>
  <th>Type</th>
  <th>Notes</th>
  <th>Date</th>
  <th>Amount</th>
  <th width=75>Actions</th>
  </thead>

  <tbody id="payment_body">
  <%= render 'payment' %>
  </tbody>
</table>


