<div class="login_container">
<h1>Sign up to Open<span>Source</span>Billing</h1>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%#= devise_error_messages! %>

  <div class="form_title login_title"><!--<%= f.label :email %>-->
  <%= f.email_field :email, :autofocus => true, :placeholder => "email", :id => "login_email" %></div>

  <div class="form_title login_title"><!--<%= f.label :password %>-->
  <%= f.password_field :password, :placeholder => "password", :id => "login_pswd" %></div>

  <div class="form_title login_title" style="margin-bottom:6px;"><!--<%= f.label :password_confirmation %>-->
  <%= f.password_field :password_confirmation, :placeholder => "password confirmation", :id => "login_pswd"  %></div>

  <div class="login_btn_container"><%= f.submit "Sign up", :class => "new_btn login_btn" %></div>
<% end %>

<!--<%= render "devise/shared/links" %>-->
</div>

<div class="login_footer">

  <a class="site_link" href="#">www.opensourcebilling.org</a>
  <p class="team_link">powered by:<a href="#"> vteams.com</a></p>

</div>
