<div class="login_container">


<h1>Sign in to Open<span>Source</span>Billing</h1>

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
<div class="form_title login_title"><!--<%= f.label :email %>-->
  <%=  f.email_field :email, :autofocus => true, :placeholder => "username", :id => "login_email", :title => "Enter your username" %></div>

  <div class="form_title login_title"><!--<%= f.label :password %>-->
  <%= f.password_field :password, :placeholder  => "password", :id => "login_pswd", :title => "Enter your password" %></div>

  <% if devise_mapping.rememberable? -%>
<div class="login_check"><%= f.check_box :remember_me %> <%= f.label (:remember_me) {"Keep me signed-in <span style='color:#ababab'> (in #{browser.name} on this machine)</span>".html_safe} %> </div>
  <% end -%>

<div class="login_btn_container"><%= f.submit "Sign in", :class => "new_btn login_btn" %></div>
<% end %>

<!--<%= render "devise/shared/links" %>-->


</div>

<div class="login_footer">

  <a class="site_link" href="#">www.opensourcebilling.org</a>
