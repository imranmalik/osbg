<%unless @tax_exists%>
    <% inlineform_action = params[:add_more] ? "inlineform:save_and_add_more" : "inlineform:save" %>
    <%= "{\"action\": \"#{inlineform_action}\",\"exists\": \"#{@tax_exists}\", \"record\": \"<option value='#{@taxis.id}' selected>#{@taxis.name}</option>\"}".html_safe%>
<%else%>
    <%= "{\"exists\": \"#{@tax_exists}\"}".html_safe%>
<%end%>